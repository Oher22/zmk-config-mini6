#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp Q    &kp W    &mo 1
                &kp A    &kp S    &kp D
            >;
        };

        bt_layer {
            bindings = <
                &bootloader     &kp E   &none
                &bt BT_SEL 0    &bt BT_SEL 1   &bt BT_SEL 2
            >;
        };
    };
};
