#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp Q    &kp W    &lt 1 E
                &kp A    &kp S    &kp D
            >;
        };

        bt_layer {
            bindings = <
                &bootloader     &kp E   &none
                &kp R     &kp F        &kp SPACE
            >;
        };
    };
};
